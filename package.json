{
  "name": "hedhog",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "author": "HedHog",
  "license": "MIT",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "test:e2e": "turbo run test:e2e",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "init:env": "npx ts-node ./scripts/init-env.ts",
    "patch": "npx ts-node ./scripts/patch.ts",
    "clean": "node ./scripts/clean",
    "postinstall": "npm run init:env",
    "db:update": "pnpm --filter @hed-hog/api-prisma prisma:generate && pnpm --filter api prisma:update"
  },
  "devDependencies": {
    "@hed-hog/eslint-config": "workspace:*",
    "@hed-hog/typescript-config": "workspace:*",
    "@types/node": "^24.0.0",
    "@types/react": "^19.1.7",
    "@types/react-dom": "^19.1.6",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "prisma": "^6.9.0",
    "ts-node": "^10.9.2",
    "turbo": "^2.5.4",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "pnpm@10.11.1",
  "dependencies": {
    "@nestjs/common": "^11",
    "@nestjs/core": "^11",
    "@nestjs/platform-express": "^11",
    "axios": "^1.9.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "date-fns": "^3.6.0",
    "express": "^4.21.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "uuid": "^11.1.0"
  }
}
