{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "composite": true,
    "allowSyntheticDefaultImports": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": false,
    "incremental": true,
    "module": "commonjs",
    "moduleResolution": "Node10",
    "noFallthroughCasesInSwitch": false,
    "noImplicitAny": false,
    "removeComments": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strictNullChecks": false,
    "strictBindCallApply": false,
    "baseUrl": ".",
    "paths": {
      "@prisma/client": ["../../packages/api-prisma/node_modules/.prisma/client"],
      "@prisma/client/*": ["../../packages/api-prisma/node_modules/.prisma/client/*"],
      "@hedhog/api": ["../../packages/api/src"],
      "@hedhog/api/*": ["../../packages/api/src/*"],
      "@hedhog/api-locale": ["../../packages/api-locale/src"],
      "@hedhog/api-locale/*": ["../../packages/api-locale/src/*"],
      "@hedhog/api-mail": ["../../packages/api-mail/src"],
      "@hedhog/api-mail/*": ["../../packages/api-mail/src/*"],
      "@hedhog/api-pagination": ["../../packages/api-pagination/src"],
      "@hedhog/api-pagination/*": ["../../packages/api-pagination/src/*"],
      "@hedhog/api-prisma": ["../../packages/api-prisma/src"],
      "@hedhog/api-prisma/*": ["../../packages/api-prisma/src/*"],
      "@hedhog/api-healthy": ["../../packages/api-healthy/src"],
      "@hedhog/api-healthy/*": ["../../packages/api-healthy/src/*"],
      "@hedhog/api-developer": ["../../packages/api-developer/src"],
      "@hedhog/api-developer/*": ["../../packages/api-developer/src/*"],

      "@hedhog/admin": ["../../libraries/admin/src"],
      "@hedhog/admin/*": ["../../libraries/admin/src/*"]
    }
  }
}
