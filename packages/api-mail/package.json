{
  "name": "@hed-hog/api-mail",
  "version": "0.0.2",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "_dev": "pnpm build --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "prebuild": "npx ts-node ../../scripts/build-dependencies.ts api-mail",
    "build": "tsc --project tsconfig.production.json",
    "patch": "npx ts-node ../../scripts/patch.ts api-mail",
    "prod": "pnpm run patch && pnpm run build && pnpm publish --access public --no-git-checks"
  },
  "dependencies": {
    "@nestjs/mapped-types": "*",
    "@nestjs/common": "*",
    "@nestjs/axios": "*",
    "@nestjs/jwt": "*",
    "axios": "*",
    "mimemessage": "1.0.5",
    "bcrypt": "^5.1.1",
    "nodemailer": "^6.9.0",
    "aws-sdk": "2.1691.0",
    "googleapis": "144.0.0",
    "rxjs": "*"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  }
}
